{
    "compilerOptions": {
      "baseUrl": "src",
      "target": "es5",
      "module": "Commonjs",
      "outDir": "./dist",
      "strict": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "rootDirs": ["src"],
    }
  }